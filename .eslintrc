{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "parser":"babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": 0,
        "linebreak-style": 0,
        "no-console": 0,
        "quotes": [
            1,
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "max-len": [1, 50, 2, {ignoreComments: true}],
        "indent": [
            1,
            2
        ],
    }
}